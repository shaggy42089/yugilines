{
  "name": "yugilines",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/elec.js",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "elec": "electron ./src/elec.js",
    "build": "vite build && electron-builder",
    "build:win": "npm run build -- --win",
    "build:mac": "npm run build -- --mac",
    "build:linux": "npm run build -- --linux"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "sass": "^1.77.6",
    "vite": "^5.1.6"
  },
  "build": {
    "appId": "com.shaggy.yugilines",
    "productName": "Yugilines",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.map",
      "!src",
      "!e2e",
      "src/main.js",
      "src/elec.js",
      "!hooks",
      "!webpack.config.js",
      "!karma.conf.js",
      "!tsconfig.json",
      "!tslint.json",
      "!typings.json",
      "!typings",
      "!*.json",
      "!*.md",
      "!*.yml",
      "!*.sh"
    ],
    "win": {
      "icon": "icon.ico"
    },
    "mac": {
      "icon": "icon.icns"
    },
    "linux": {
      "icon": "icons"
    }
  }
}
